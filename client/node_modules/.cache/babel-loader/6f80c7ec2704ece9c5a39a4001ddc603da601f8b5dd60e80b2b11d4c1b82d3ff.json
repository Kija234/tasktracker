{"ast":null,"code":"import Tasks from '../components/Tasks';\nimport AddTask from '../components/AddTask';\nimport Header from '../components/Header';\nimport { useTaskStore } from '../stores/TaskStore';\nexport default {\n  name: 'Tasks1',\n  components: {\n    Tasks,\n    AddTask,\n    Header\n  },\n  data() {\n    return {\n      tasks: [],\n      showAddTask: false\n    };\n  },\n  methods: {\n    toggleAddTask() {\n      this.showAddTask = !this.showAddTask;\n    },\n    setup() {\n      const taskStore = useTaskStore();\n      //fetch data\n      taskStore.getTask();\n      return {\n        taskStore\n      };\n    },\n    async AddTask(task) {\n      await axios.put('/api/tasks').then(response => {\n        this.tasks = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  async deleteTask(id) {\n    if (confirm('Are you sure?')) {\n      const res = await fetch(`/api/tasks/${id}`, {\n        method: 'DELETE'\n      });\n      res.status === 200 ? this.tasks = this.tasks.filter(task => task.id !== id) : alert('Error deleting task');\n    }\n  }\n}; // Ende von methods","map":{"version":3,"names":["Tasks","AddTask","Header","useTaskStore","name","components","data","tasks","showAddTask","methods","toggleAddTask","setup","taskStore","getTask","task","axios","put","then","response","catch","error","console","log","deleteTask","id","confirm","res","fetch","method","status","filter","alert"],"sources":["/Users/dittmannja/dev/UÌˆbungen/TaskTrackerApp/nestjs_travery/TaskTracker-1/nest-rest-api/client/src/views/Tasks1.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"task-list\">\n      <Header :showAddTask=\"showAddTask\" />\n      <AddTask v-show=\"showAddTask\" @add-task=\"addTask\" />\n      <Tasks @delete-task=\"deleteTask\" :tasks=\"tasks\" />\n      <router-view></router-view>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Tasks from '../components/Tasks';\nimport AddTask from '../components/AddTask';\nimport Header from '../components/Header';\nimport { useTaskStore } from '../stores/TaskStore';\n\nexport default {\n  name: 'Tasks1',\n\n  components: {\n    Tasks,\n    AddTask,\n    Header,\n  },\n  data() {\n    return {\n      tasks: [],\n      showAddTask: false,\n    };\n  },\n\n  methods: {\n    toggleAddTask() {\n      this.showAddTask = !this.showAddTask;\n    },\n\n    setup() {\n      const taskStore = useTaskStore();\n      //fetch data\n      taskStore.getTask();\n      return { taskStore };\n    },\n\n    async AddTask(task) {\n      await axios\n        .put('/api/tasks')\n        .then((response) => {\n          this.tasks = response.data;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n\n  async deleteTask(id) {\n    if (confirm('Are you sure?')) {\n      const res = await fetch(`/api/tasks/${id}`, {\n        method: 'DELETE',\n      });\n      res.status === 200\n        ? (this.tasks = this.tasks.filter((task) => task.id !== id))\n        : alert('Error deleting task');\n    }\n  },\n}; // Ende von methods\n</script>"],"mappings":"AAYA,OAAOA,KAAI,MAAO,qBAAqB;AACvC,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,MAAK,MAAO,sBAAsB;AACzC,SAASC,YAAW,QAAS,qBAAqB;AAElD,eAAe;EACbC,IAAI,EAAE,QAAQ;EAEdC,UAAU,EAAE;IACVL,KAAK;IACLC,OAAO;IACPC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACF,WAAU,GAAI,CAAC,IAAI,CAACA,WAAW;IACtC,CAAC;IAEDG,KAAKA,CAAA,EAAG;MACN,MAAMC,SAAQ,GAAIT,YAAY,CAAC,CAAC;MAChC;MACAS,SAAS,CAACC,OAAO,CAAC,CAAC;MACnB,OAAO;QAAED;MAAU,CAAC;IACtB,CAAC;IAED,MAAMX,OAAOA,CAACa,IAAI,EAAE;MAClB,MAAMC,KAAI,CACPC,GAAG,CAAC,YAAY,EAChBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACX,KAAI,GAAIW,QAAQ,CAACZ,IAAI;MAC5B,CAAC,EACAa,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACN;EACF,CAAC;EAED,MAAMG,UAAUA,CAACC,EAAE,EAAE;IACnB,IAAIC,OAAO,CAAC,eAAe,CAAC,EAAE;MAC5B,MAAMC,GAAE,GAAI,MAAMC,KAA