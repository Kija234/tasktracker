{"ast":null,"code":"import Contacts from \"../components/Contacts\";\nimport AddContact from \"../components/addContact\";\nimport ButtonC from \"../components/ButtonC\";\nimport axios from \"axios\";\nexport default {\n  name: \"Contact1\",\n  props: {},\n  components: {\n    Contacts,\n    AddContact,\n    ButtonC\n  },\n  data() {\n    return {\n      contacts: [],\n      showAddContact: false\n    };\n  },\n  methods: {\n    toggleAddContact() {\n      this.showAddContact = !this.showAddContact;\n    },\n    async aDdContact(contact) {\n      const res = await axios.get(\"api/contacts\", {\n        method: \"POST\",\n        headers: {\n          \"Content-type\": \"application/json\"\n        },\n        body: JSON.stringify(contact)\n      });\n      const data = await res.json();\n      this.contacts = [...this.contacts, data];\n    },\n    async deleteContact(id) {\n      if (confirm(\"Are you sure?\")) {\n        const res = await axios.get(`api/contacts/${id}`, {\n          method: \"DELETE\"\n        });\n        res.status === 200 ? this.contacts = this.contacts.filter(contact => contact.id !== id) : alert(\"Error deleting contacts\");\n      }\n    }\n  },\n  async created() {\n    try {\n      const res = await axios.get(`api/contacts`);\n      this.contacts = res.data;\n    } catch (error) {\n      console.log(error);\n    }\n  }\n};","map":{"version":3,"names":["Contacts","AddContact","ButtonC","axios","name","props","components","data","contacts","showAddContact","methods","toggleAddContact","aDdContact","contact","res","get","method","headers","body","JSON","stringify","json","deleteContact","id","confirm","status","filter","alert","created","error","console","log"],"sources":["/Users/dittmannja/dev/UÌˆbungen/TaskTrackerApp/nestjs_travery/TaskTracker-1/nest-rest-api/client/src/views/Contact1.vue"],"sourcesContent":["<template>\n  <div>\n    <ButtonC\n      @toggle-add-contact=\"toggleAddContact\"\n      title=\"Contacts\"\n      :showAddContact=\"showAddContact\"\n    />\n\n    <addContact v-show=\"showAddContact\" @add-contact=\"aDdContact\" />\n\n    <Contacts @delete-contact=\"deleteContact\" :contacts=\"contacts\" />\n    <router-view :showAddContact=\"showAddContact\"></router-view>\n    <router-link to=\"/\">Go Back</router-link>\n  </div>\n</template>\n\n<script>\nimport Contacts from \"../components/Contacts\";\nimport AddContact from \"../components/addContact\";\nimport ButtonC from \"../components/ButtonC\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"Contact1\",\n  props: {},\n  components: {\n    Contacts,\n    AddContact,\n    ButtonC,\n  },\n  data() {\n    return {\n      contacts: [],\n      showAddContact: false,\n    };\n  },\n  methods: {\n    toggleAddContact() {\n      this.showAddContact = !this.showAddContact;\n    },\n    async aDdContact(contact) {\n      const res = await axios.get(\"api/contacts\", {\n        method: \"POST\",\n        headers: {\n          \"Content-type\": \"application/json\",\n        },\n        body: JSON.stringify(contact),\n      });\n      const data = await res.json();\n      this.contacts = [...this.contacts, data];\n    },\n    async deleteContact(id) {\n      if (confirm(\"Are you sure?\")) {\n        const res = await axios.get(`api/contacts/${id}`, {\n          method: \"DELETE\",\n        });\n        res.status === 200\n          ? (this.contacts = this.contacts.filter(\n              (contact) => contact.id !== id\n            ))\n          : alert(\"Error deleting contacts\");\n      }\n    },\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(`api/contacts`);\n      this.contacts = res.data;\n    } catch (error) {\n      console.log(error);\n    }\n  },\n};\n</script>"],"mappings":"AAiBA,OAAOA,QAAO,MAAO,wBAAwB;AAC7C,OAAOC,UAAS,MAAO,0BAA0B;AACjD,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CAAC,CAAC;EACTC,UAAU,EAAE;IACVN,QAAQ;IACRC,UAAU;IACVC;EACF,CAAC;